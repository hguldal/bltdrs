    if (!WebSecurity.IsAuthenticated)
   
        {
            Response.StatusCode=401;
            Response.End();
                   
        }  

    var GorevID=Request["GorevID"];
    

    if (string.IsNullOrEmpty(GorevID))
     {
    
       Response.StatusCode=500;
       Response.End();
     }


     var db=Database.Open("fb1ae");
 
  
     db.Execute("UPDATE Gorev SET Silindi=1 WHERE GorevID=@0 AND UserID=@1",GorevID,WebSecurity.CurrentUserId);
        
     db.Close();
     Response.Write("OK");
     Response.End();
    
    
}



