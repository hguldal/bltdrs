@{
  if (!WebSecurity.IsAuthenticated)
   
        {
            Response.StatusCode=401;
            Response.End();
                   
        }  

    var DosyaID=Request["DosyaID"];
    var UserID=Request["UserID"];
    

    if (string.IsNullOrEmpty(DosyaID) || string.IsNullOrEmpty(UserID))
     {
    
       Response.StatusCode=500;
       Response.End();
     }

     var db=Database.Open("fb1ae");
 
     var queryDosya=db.QuerySingle("SELECT * FROM Dosya WHERE DosyaID=@0 AND UserID=@1",DosyaID,WebSecurity.CurrentUserId);

    if (WebSecurity.CurrentUserId!=queryDosya.UserID)
    {
        Response.StatusCode=400;
        Response.End();
    }

     db.Execute("DELETE FROM DosyaIzinKisi WHERE DosyaID=@0 AND UserID=@1",DosyaID,UserID);
     db.Execute("UPDATE Bildirim SET Silindi=1 WHERE IliskiliOge=@0","dsyeik" + UserID.ToString() + DosyaID.ToString());
     db.Close();
     Response.Write("OK");
     Response.End();
     
    
     }
